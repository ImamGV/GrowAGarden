local EasterShop = Tabs.Main:AddRightGroupbox("Easter Shop")
function BuyEaster(name)
local args = {
    [1] = name
}

game:GetService("ReplicatedStorage").GameEvents.BuyEasterStock:FireServer(unpack(args))
end
